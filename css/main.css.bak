@media only screen and (max-width: 460px)
{
	body{
	}
	#start{
		position : absolute;
		top : 31%;
		left : 15%;
		width : 70%;
		height : 50%
	}
	#ques{
		position : absolute;
		top : 10%;
		left : 15%;
		width : 70%;
		height : 30%;
	}
	#sol{
		position : absolute;
		top : 50%;
		left : 15%;
		width : 70%;
		height : 30%;
	}
	img{
		width : 100%;
		height : 100%;
	}
	#btnM{
		padding-top : 2%;
	}
	#list{
		font-family : Comic Sans MS;
		color : rgb(26,82,118); 
		font-size : 20px; 
		text-align : center;
	}
	#txt{
		font-family : Maiandra GD;
		font-size : 200%;
		color : white;
		text-indent : 10%;
	}
	#solt{
		display : block;
		color : white;
		font-family : Maiandra GD;
		font-size : 200%;
		text-align : left;
		float : left;
	}
	hr{
		color : green;
	}
	#sideHeading{
		font-family : Sitka Small;
		font-size  : 150%;
		color : rgb(26,82,118);
	}
	#picsT{
		padding-left : 20%;
		width : 80%;
	}
	i{
		color : olive;
	}
	strong{
		color : orange;
	}
	table{
		border-collapse :collapse;
		width : 100%;
	}
	th, td {
		border-style : solid;
		border-color : green;
	}
	#info{
		position : absolute;
		top : 20%;
		text-align : center;
		font-family : Cambria Math;
		font-size : 150%;
	}
}
